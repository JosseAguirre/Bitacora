	public void guardardetalletransferenciain() {
		BovDetalleTransferencia bovdetalletransferencia = new BovDetalleTransferencia();

		for (BovDetalleTransferencia detalleTransferecia : listaDetalleTransferencia) {
			bovdetalletransferencia.setIdDetalleTransferencia(serviciodetalletransferencia.getPK());
			bovdetalletransferencia.setIdbovtransferencia(bovcabezeratransferencia);
			bovdetalletransferencia.setCantidad(detalleTransferecia.getCantidad());
			bovdetalletransferencia.setSubtotal(detalleTransferecia.getSubtotal());
			bovdetalletransferencia.setIdespecie(detalleTransferecia.getIdespecie());

			serviciodetalletransferencia.create(bovdetalletransferencia);
		}
	}


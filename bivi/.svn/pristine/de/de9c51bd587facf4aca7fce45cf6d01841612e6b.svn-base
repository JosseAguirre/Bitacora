		serviciobovcabezeratransferencia.create(bovcabezeratransferencia);

		buscaIdCombosInt();
		// mensaje al eliminar bovtransferencia
		FacesContext.getCurrentInstance().addMessage(null, new FacesMessage(FacesMessage.SEVERITY_INFO, "Aviso",
				"Se eliminó la  Transferencia: " + bovcabezeratransferencia.getIdCabezeratransferencia()));

		cancelar();
		admdetallecatalogo = serviciodetallecatalogo.findOne(305);
		cetipotransferencia = serviciocetipotransferencia.findOne(1);
		bovtipotransaccion = serviciobovtipotransaccion.findOne(2);
		bovcabezeratransferencia.setIdtipotransferencia(cetipotransferencia);
		bovcabezeratransferencia.setIdtipotransaccion(bovtipotransaccion);
		bovcabezeratransferencia.setNumeroticket(serviciobovcabezeratransferencia.getnumerotiket());
		bovcabezeratransferencia.setId_estadotrasferencia(admdetallecatalogo);
		bovcabezeratransferencia.setFecha(date);
		bovcabezeratransferencia.setHora(time);
		buscaIdCombosInt();
		serviciobovcabezeratransferencia.create(bovcabezeratransferencia);

	}

